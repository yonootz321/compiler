{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES6",
    "outDir": "./",
    "moduleResolution": "node",
    "module": "commonjs",
    "allowJs": true,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "newLine": "CRLF",
    "resolveJsonModule": true
  },
  "include": [
    "./*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}